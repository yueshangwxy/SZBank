:data=\"linkDataList\" height=\"235px\">\n          <el-table-column label=\"序号\" type=\"index\" width=\"50\">\n          </el-table-column>\n          <el-table-column\n              v-for=\"(item, index) in labLinkDataProp\"\n              :prop=\"item.prop\"\n              :label=\"item.label\"\n              :align=\"item.align\"\n              :key=\"index\"\n              :min-width=\"item.width\"\n              show-overflow-tooltip\n          >\n            <template scope=\"scope\">\n              <span v-if=\"item.type === 'tag'\">\n                <el-tag :type=\"item.tagType(scope.row)\" size=\"mini\">\n                  {{ item.render(scope.row) }}\n                </el-tag>\n              </span>\n              <span v-else>{{ scope.row[item.prop] }}</span>\n            </template>\n          </el-table-column>\n        </el-table>\n      </el-dialog>\n      <!-- 流程详情 -->\n      <ProcessDetails\n        :tableId=\"tableId\"\n        :detailVis.sync=\"detailVis\"\n        @handleClose=\"handleClose\"\n      >\n      </ProcessDetails>\n      <!-- API详情 -->\n      <ApiDetails\n        :apiId=\"apiId\"\n        :apiDetailVis.sync=\"apiDetailVis\"\n        @handleClose=\"handleClose\"\n      >\n      </ApiDetails>\n      <!-- 实验室详情 -->\n      <el-dialog :visible=\"labDetailVis\" title=\"详情\" @close=\"handleClose\">\n        <div\n          style=\"\n            overflow-y: auto;\n            overflow-x: hidden;\n            height: 100%;\n            padding-left: 10px;\n            padding-right: 10px;\n          \"\n        >\n          <el-form\n            :model=\"labDetail\"\n            label-position=\"left\"\n            label-width=\"100px\"\n            size=\"medium\"\n          >\n            <el-form-item label=\"表或视名(包含项目)\">\n              <el-input v-model=\"labDetail.tableName\" disabled></el-input>\n            </el-form-item>\n            <el-form-item label=\"申请口径\">\n              <el-input\n                type=\"textarea\"\n                v-model=\"labDetail.sqlText\"\n                disabled\n                :rows=\"3\"\n              ></el-input>\n            </el-form-item>\n            <el-form-item label=\"申请说明\">\n              <el-input\n                v-model=\"labDetail.applyDescription\"\n                disabled\n              ></el-input>\n            </el-form-item>\n            <el-form-item label=\"申请人\">\n              <el-input v-model=\"labDetail.applyUserName\" disabled></el-input>\n            </el-form-item>\n            <el-form-item label=\"申请人部门\">\n              <el-input v-model=\"labDetail.applyDeptName\" disabled></el-input>\n            </el-form-item>\n            <el-form-item label=\"申请时间\">\n              <el-date-picker\n                type=\"datetime\"\n                v-model=\"labDetail.applyTime\"\n                disabled\n              ></el-date-picker>\n            </el-form-item>\n            <el-form-item label=\"申请状态\">\n              <el-input v-model=\"labDetail.applyStatusShow\" disabled></el-input>\n            </el-form-item>\n<!--            <el-form-item>\n              <el-button type=\"info\" @click=\"handleClose\">关闭 </el-button>\n            </el-form-item>-->\n          </el-form>\n        </div>\n      </el-dialog>\n\n      <!-- 实验室审批 -->\n      <el-dialog\n        class=\"approve\"\n        :visible.sync=\"labApproveDialog\"\n        title=\"审批\"\n        width=\"400px\"\n        top=\"38vh\"\n      >\n        <el-form\n          :model=\"ruleForm\"\n          :rules=\"rules\"\n          class=\"demo-ruleForm\"\n          ref=\"ruleForm\"\n        >\n          <!-- 审批意见 -->\n          <el-form-item label=\"审批意见\" prop=\"apprComment\">\n            <el-input\n              v-model=\"ruleForm.apprComment\"\n              placeholder=\"请输入审批意见\"\n              type=\"textarea\"\n              rows=\"4\"\n              resize=\"none\"\n              maxlength=\"1000\"\n              clearable\n            ></el-input>\n          </el-form-item>\n        </el-form>\n        <span slot=\"footer\" class=\"dialog-footer\">\n          <el-button @click=\"labReject('ruleForm')\">拒绝</el-button>\n          <el-button type=\"primary\" @click=\"labPass('ruleForm')\"\n            >通过</el-button\n          >\n        </span>\n      </el-dialog>\n    </div>\n    <!-- 底部 -->\n    <div class=\"portal-footer\"></div>\n  </div>\n",null]}