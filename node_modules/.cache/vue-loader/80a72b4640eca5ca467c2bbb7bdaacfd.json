aceholder=\"申请时间\">\n                    </el-date-picker> -->\n                <el-button @click=\"initList()\">搜索</el-button>\n            </div>\n            <!-- 表格 -->\n            <div class=\"table\">\n                <el-table height=\"300\"\n                    :data=\"tableData\"\n                    max-height=\"543px\">\n                    <el-table-column\n                        v-for=\"(item,index) in propList\"\n                        :key=\"index\"\n                        :prop=\"item.prop\"\n                        :label=\"item.label\"\n                        :width=\"item.width\"\n                        :align=\"item.align\">\n                        <template slot-scope=\"scope\">\n                            <span v-if=\"item.type==='tag'\">\n                                <el-tag :type=\"item.tagType(scope.row)\"\n                                    size=\"mini\">\n                                    {{scope.row[item.prop]}}\n                                </el-tag>\n                            </span>\n                            <span v-else>{{scope.row[item.prop]}}</span>\n                        </template>\n                    </el-table-column>\n                    <el-table-column\n                        label=\"操作\"\n                        width=\"150\">\n                        <template slot-scope=\"scope\">\n                            <el-button type=\"text\" @click=\"viewDetails(scope.row)\">详情</el-button>\n                            <el-button type=\"text\" @click=\"approveBtn(scope.row)\" v-if=\"radio==1\">审批</el-button>\n                            <el-button type=\"text\" @click=\"appService(scope.row)\" v-if=\"activeTab==='service'\">应用服务关系</el-button>\n                        </template>\n                    </el-table-column>\n                </el-table>\n            </div>\n            <!-- 分页 -->\n            <div class=\"pagination\">\n                <el-pagination \n                    @current-change=\"handl