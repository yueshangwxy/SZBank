\r\n                this.searchInput = ''\r\n                this.$router.push({\r\n                    path: '/searchPage/index',\r\n                })\r\n            },\r\n            // 单个标签删除\r\n            handleClose(tag) {\r\n                this.historyTags.splice(this.historyTags.indexOf(tag), 1);\r\n                localStorage.setItem('historyTags',JSON.stringify(this.historyTags))\r\n            },\r\n            // 全部删除\r\n            clearAllHistory() {\r\n                this.historyTags = []\r\n                localStorage.setItem('historyTags',JSON.stringify(this.historyTags))\r\n            }\r\n        }\r\n    }\r\n</script>a\r\n<style lang=\"scss\">\r\n    .panelGroup {\r\n        width: 100%;\r\n        height: 100%;\r\n        padding-top: 5%;\r\n        display: flex;\r\n        flex-direction: column;\r\n\r\n        // 搜索框\r\n        .search {\r\n            display: flex;\r\n\r\n            .el-input {\r\n                width: 515px;\r\n                height: 40px;\r\n\r\n                .el-input__inner {\r\n                    height: 100%;\r\n                }\r\n\r\n                .el-button {\r\n                    width: 75px;\r\n                    height: 40px;\r\n                    font-size: 16px;\r\n                    background: #84C22E;\r\n                    color: #fff;\r\n                }\r\n            }\r\n        }\r\n\r\n        // 历史记录\r\n        .history {\r\n            width: 500px;\r\n\r\n            h5 {\r\n                display: flex;\r\n                justify-content: space-between;\r\n                align-items: center;\r\n                color: #84C22E;\r\n                span {\r\n                    font-weight: 400;\r\n                }\r\n            }\r\n\r\n            .historyList {\r\n                max-height: 150px;\r\n                overflow: hidden;\r\n                .el-tag {\r\n                    height: 30px;\r\n                    margin: 5px;\r\n                    line-height: 28